{
 "manifest_version": 2,
 "name": "WebiTrust",
 "description": "Extension permettant de définir un indice sureté d'un site",
 "version": "1.0",
 "permissions": [
  "http://*/*",
  "https://*/*"
 ],
 "browser_action": {
  "default_icon" : "img/icon.png",
  "default_popup": "popup.html"
 },
 "icons": {
  "128" : "img/icon_128.png"
 }
}